routing-options {
    interface-routes {
        rib-group inet Connected;
    }
    static {
        rib-group Default;
        route 10.0.0.0/8 next-hop 10.249.100.253;
        route 10.249.200.0/24 next-hop 10.249.101.253;
        route 10.249.201.0/25 discard;
    }
    rib-groups {
        /* install static routes (see above) in these tables */
        Default {
            /* import Global in cust routing-instances */
            import-rib [ inet.0 DYMON.inet.0 ];
        }
        /* install connected routes in these tables */
        Connected {
            import-rib [ DYMON.inet.0 inet.0 SFB_PPMT-vr.inet.0 ];
        }
        /* install default routes (see bgp conf) in these tables */
        Gateway {
            import-rib [ inet.0 DYMON.inet.0 NTT_NEW_DESIGN.inet.0 SFB_PPMT-vr.inet.0 ];
            import-policy Gateway;
        }
    }
    autonomous-system 65151;
}
protocols {
    bgp {
        log-updown;
        family inet {
            unicast {
                rib-group Gateway;
            }
        }
        group INET {
            type external;
            export AS_Network;
            neighbor 172.31.253.25 {
                peer-as 200077;
            }
            neighbor 172.31.253.29 {
                peer-as 200077;
            }
        }
    }
}
policy-options {
    prefix-list Local {
        185.37.221.128/25;
    }
    prefix-list DYMON {
        172.19.129.0/27;
    }
    prefix-list MT {
        172.19.128.128/27;
    }
    prefix-list PFX_OSPF-MCAST_V4 {
        224.0.0.5/32;
        224.0.0.6/32;
    }
    prefix-list MGMT_NETS_V4 {
        10.0.1.0/24;
        10.100.19.20/32;
        10.100.19.21/32;
        10.100.137.89/32;
        10.115.137.89/32;
        10.245.2.0/24;
        10.250.137.89/32;
        172.29.0.0/16;
        172.30.0.0/16;
    }
    prefix-list NETCONF_SERVERS_V4 {
        10.100.19.21/32;
        10.100.137.89/32;
        10.115.137.89/32;
        10.250.137.89/32;
    }
    prefix-list LOCAL_MGMT_V4 {
        apply-path "interfaces fxp0 unit <*> family inet address <*.*.*.*>";
    }
    prefix-list LOCAL_IPV4 {
        apply-path "interfaces <*> unit <*> family inet address <*.*.*.*>";
    }
    prefix-list BGP_PEERS_V4 {
        apply-path "protocols bgp group <*> neighbor <*.*.*.*>";
    }
    prefix-list BGP_VRF_PEERS_V4 {
        apply-path "routing-instances <*> protocols bgp group <*> neighbor <*.*.*.*>";
    }
    prefix-list NTP_SERVERS_V4 {
        apply-path "system ntp server <*.*.*.*>";
    }
    prefix-list SNMP_CLIENT_LISTS_V4 {
        apply-path "snmp client-list <*> <*.*.*.*>";
    }
    prefix-list PFX_RADIUS-SERVERS_V4 {
        apply-path "system radius-server <*.*.*.*>";
    }
    prefix-list PFX_IPSEC-VPN_PEER_V4 {
        apply-path "security ike gateway <*> address <*.*.*.*>";
    }
    policy-statement AS_Network {
        term Network {
            from {
                prefix-list Local;
            }
            then accept;
        }
        then reject;
    }
    policy-statement Connected {
        term Lync_Global {
            from {
                prefix-list DYMON;
                prefix-list MT;
            }
            then accept;
        }
        then reject;
    }
    policy-statement Gateway {
        term Import_Gateway {
            from {
                protocol bgp;
                route-filter 0.0.0.0/0 exact;
            }
            then accept;
        }
        then reject;
    }
}
