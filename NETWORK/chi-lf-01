    bgp {
        log-updown;
        group BORDER_V4 {
            type external;
            log-updown;
            family inet {
                unicast;
            }
            export PS_EXP_BORDER_V4;
            peer-as 53550;
            multipath multiple-as;
            neighbor 172.22.0.14;
            neighbor 172.22.0.18;
        }
        group CORE_V4 {
            type external;
            import PS_IMP_CORE_V4;
            family inet {
                unicast;
            }
            export PS_EXP_CORE_V4;
            peer-as 65117;
            neighbor 172.22.2.9;
            neighbor 172.22.2.11;
        }
        group BORDER_V6 {
            type external;
            hold-time 10;
            import PS_IMP_BORDER_V6;
            family inet6 {
                unicast;
            }
            export PS_EXP_BORDER_V6;
            neighbor 2a00:fa60:25:2111::1 {
                description chi-bl-01;
