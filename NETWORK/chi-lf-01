policy-options {
    prefix-list PFX_CHI_FABRIC-P2P_V4 {
        172.22.0.0/23;
    }
    prefix-list PFX_CHI_INFRA-P2P_V4 {
        172.22.2.0/23;
    }
    policy-statement PS_ECMP {
        then {
            load-balance per-packet;
        }
    }
    policy-statement PS_EXP_BORDER_V4 {
        term ORIGIN {
            from {
                protocol [ direct static ];
                route-filter 10.127.0.0/16 prefix-length-range /23-/31;
            }
            then accept;
        }
        term LOOPBACK {
            from {
                protocol direct;
                route-filter 192.168.0.0/24 prefix-length-range /32-/32;
            }
            then accept;
        }
        term INFRA {
            from {
                protocol direct;
                prefix-list-filter PFX_CHI_INFRA-P2P_V4 orlonger;
            }
            then accept;
        }
        term BASTION {
            from {
                protocol direct;
                route-filter 10.0.1.4/30 exact;
            }
            then accept;
        }
        term CHI-COGENT_V4 {
            from {
                protocol direct;
                route-filter 38.65.194.0/24 orlonger;
            }
            then accept;
        }
    }
    policy-statement PS_EXP_BORDER_V6 {
        then accept;
    }
    policy-statement PS_EXP_CORE_V4 {
        term BBONE_PFXS {
            from community COMM_BBONE_PFXS;
            then {
                community delete COMM_BBONE_PFXS;
                accept;
            }
        }
        term FABRIC_AGGREGATE {
            from protocol aggregate;
            then accept;
        }
        term BASTION {
            from {
                protocol direct;
                route-filter 10.0.1.4/30 exact;
            }
            then accept;
        }
        term INFRA {
            from {
                protocol direct;
                prefix-list-filter PFX_CHI_FABRIC-P2P_V4 orlonger;
                prefix-list-filter PFX_CHI_INFRA-P2P_V4 orlonger;
            }
            then accept;
        }
        term REJECT {
            then reject;
        }
    }
    policy-statement PS_IMP_BORDER_V6 {
        then accept;
    }
    policy-statement PS_IMP_CORE_V4 {
        term SET_COMM {
            from as-path-group ASN_CHI_LEGACY;
            then {
                community add COMM_CHI_LEGACY;
                accept;
            }
        }
        term REJECT_ALL {
            then reject;
        }
    }
    policy-statement PS_SET_LP {
        /* rfc8326 - Graceful BGP Session Shutdown */
        term GRACEFUL_SHUT {
            from community COMM_GRACEFUL_SHUT;
            then {
                local-preference 0;
                next policy;
            }
        }
        term LP_80 {
            from community COMM_LP80;
            then {
                local-preference 80;
                next policy;
            }
        }
        term LP_120 {
            from community COMM_LP120;
            then {
                local-preference 120;
                next policy;
            }
        }
    }
    community COMM_BBONE_PFXS members 64100:100;
    community COMM_CHI_LEGACY members 64100:101;
    community COMM_GRACEFUL_SHUT members 65535:0;
    community COMM_LP120 members 53550:120;
    community COMM_LP80 members 53550:80;
    as-path ASN_SCH "^65110$";
    as-path AS_CHI_LEGACY "^65117$";
    as-path-group ASN_CHI_LEGACY {
        as-path CORE_65117 "^65117$";
        as-path SCH_65110 ".*65110$";
    }
}
