firewall {
    family inet {
        filter ACCEPT_SSH_V4 {
            term ACCEPT_SSH_V4 {
                from {
                    source-prefix-list {
                        MGMT_NETS_V4;
                    }
                    protocol tcp;
                    destination-port ssh;
                }
                then {
                    count ACCEPT_SSH_V4;
                    accept;
                }
            }
        }
        filter MGMT_DISCARD_V4 {
            term DISCARD_SSH_V4 {
                from {
                    protocol tcp;
                    destination-port ssh;
                }
                then {
                    count DISCARD_SSH_V4;
                    discard;
                }
            }
        }
        filter MGMT_RESTRICT_V4 {
            term ACCEPT_SSH_V4 {
                from {
                    source-prefix-list {
                        MGMT_NETS_V4;
                    }
                    protocol tcp;
                    destination-port ssh;
                }
                then {
                    count ACCEPT_SSH_V4;
                    accept;
                }
            }
        }
        filter ACCEPT_ALL_V4 {
            term ACCEPT_ALL_V4 {
                then accept;
            }
        }
    }
    family inet6 {
        filter ACCEPT_BGP_V6 {
            term ACCEPT_BGP_V6 {
                from {
                    source-prefix-list {
                        BGP_NEIGHBORS_V6;
                        BGP_NEIGHBORS_VRF_V6;
                    }
                    destination-prefix-list {
                        LOCAL_IPV6;
                    }
                    ##
                    ## Warning: value payload-protocol ignored: unsupported platform (qfx5100-48s-6q)
                    ##
                    payload-protocol tcp;
                    ##
                    ## Warning: value port ignored: unsupported platform (qfx5100-48s-6q)
                    ##
                    port bgp;
                }
                then {
                    count ACCEPT_BGP_V6;
                    accept;
                }
            }
        }
        filter ACCEPT_BFD_V6 {
            term ACCEPT_BFD_V6 {
                from {
                    destination-prefix-list {
                        LOCAL_IPV6;
                    }
                    ##
                    ## Warning: value payload-protocol ignored: unsupported platform (qfx5100-48s-6q)
                    ##
                    payload-protocol udp;
                    ##
                    ## Warning: value port ignored: unsupported platform (qfx5100-48s-6q)
                    ##
                    port [ 3784 4784 ];
                }
                then {
                    count ACCEPT_BFD_V6;
                    accept;
                }
            }
        }
        filter ACCEPT_VRRP_V6 {
            term ACCEPT_VRRP_V6 {
                from {
                    source-prefix-list {
                        LOCAL_IPV6;
                    }
                    destination-prefix-list {
                        VRRP_V6;
                    }
                    ##
                    ## Warning: value payload-protocol ignored: unsupported platform (qfx5100-48s-6q)
                    ##
                    payload-protocol [ vrrp ah ];
                }
                then {
                    count ACCEPT_VRRP_V6;
                    accept;
                }
            }
        }
        filter ACCEPT_ICMP_V6 {
            term NO_ICMP_FRAGMENTS_V6 {
                from {
                    next-header fragment;
                    ##
                    ## Warning: value payload-protocol ignored: unsupported platform (qfx5100-48s-6q)
                    ##
                    payload-protocol icmp6;
                }
                then {
                    count NO_ICMP6_FRAGMENTS_V6;
                    log;
                    discard;
                }
            }
            term ACCEPT_ICMP_V6 {
                from {
                    ##
                    ## Warning: value payload-protocol ignored: unsupported platform (qfx5100-48s-6q)
                    ##
                    payload-protocol icmp6;
                }
                then {
                    policer 5MB_CP_POLICER;
                    count ACCEPT_ICMP_V6;
                    accept;
                }
            }
        }
        filter ACCEPT_TRACERT_V6 {
            term ACCEPT_TRACERT_UDP_V6 {
                from {
                    destination-prefix-list {
                        LOCAL_IPV6;
                    }
                    ##
                    ## Warning: value payload-protocol ignored: unsupported platform (qfx5100-48s-6q)
                    ##
                    payload-protocol udp;
                    destination-port 33435-33450;
                    hop-limit 1;
                }
                then {
                    policer 5MB_CP_POLICER;
                    count ACCEPT_TRACERT_UDP_V6;
                    accept;
                }
            }
            term ACCEPT_TRACERT_ICMP_V6 {
                from {
                    destination-prefix-list {
                        LOCAL_IPV6;
                    }
                    ##
                    ## Warning: value payload-protocol ignored: unsupported platform (qfx5100-48s-6q)
                    ##
                    payload-protocol icmp6;
                    icmp-type [ echo-request node-information-request time-exceeded ];
                    hop-limit 1;
                }
                then {
                    policer 5MB_CP_POLICER;
                    count ACCEPT_TRACERT_ICMP_V6;
                    accept;
                }
            }
        }
        filter DISCARD_ALL_V6 {
            term DISCARD_ALL {
                then discard;
            }
        }
        filter ACCEPT_MGMT_SERVICES_V6 {
            term ACCEPT_SSH_V6 {
                filter ACCEPT_SSH_V6;
            }
            term ACCEPT_TRACERT_V6 {
                filter ACCEPT_TRACERT_V6;
            }
            term ACCEPT_ICMP_V6 {
                filter ACCEPT_ICMP_V6;
            }
        }
        filter ACCEPT_NETWORK_SERVICES_V6 {
            term ACCEPT_BGP_V6 {
                filter ACCEPT_BGP_V6;
            }
            term ACCEPT_BFD_V6 {
                filter ACCEPT_BFD_V6;
            }
            term ACCEPT_VRRP_V6 {
                filter ACCEPT_VRRP_V6;
            }
            term ACCEPT_LDP_V6 {
                filter ACCEPT_LDP_V6;
            }
        }
        filter ACCEPT_SSH_V6 {
            term ACCEPT_SSH_V6 {
                from {
                    source-prefix-list {
                        MGMT_NETS_V6;
                    }
                    destination-port ssh;
                }
                then {
                    count ACCEPT_SSH_V6;
                    accept;
                }
            }
        }
        filter ACCEPT_LDP_V6 {
            term ACCEPT_LDP_DISCOVER_V6 {
                from {
                    source-prefix-list {
                        LOCAL_IPV6;
                    }
                    destination-prefix-list {
                        MCAST_ALL_ROUTERS_V6;
                    }
                    ##
                    ## Warning: value payload-protocol ignored: unsupported platform (qfx5100-48s-6q)
                    ##
                    payload-protocol udp;
                    destination-port ldp;
                }
                then accept;
            }
            term ACCEPT_LDP_UCAST_V6 {
                from {
                    destination-prefix-list {
                        LOCAL_IPV6;
                    }
                    ##
                    ## Warning: value payload-protocol ignored: unsupported platform (qfx5100-48s-6q)
                    ##
                    payload-protocol tcp;
                    ##
                    ## Warning: value port ignored: unsupported platform (qfx5100-48s-6q)
                    ##
                    port ldp;
                }
                then count ACCEPT_LDP_UCAST;
            }
            term ACCEPT_TLDP_DISCOVER_V6 {
                from {
                    destination-prefix-list {
                        LOCAL_IPV6;
                    }
                    ##
                    ## Warning: value payload-protocol ignored: unsupported platform (qfx5100-48s-6q)
                    ##
                    payload-protocol udp;
                    destination-port ldp;
                }
                then {
                    count ACCEPT_TLDP_DISCOVER;
                    accept;
                }
            }
        }
    }
    policer 5MB_CP_POLICER {
        if-exceeding {
            bandwidth-limit 5m;
            burst-size-limit 1m;
        }
        then discard;
    }
}
