    }
    policy-statement PS_IMP_NTTGN_V4 {
        term ACCEPT {
            then {
                local-preference 200;
                accept;
            }
        }
    }
    policy-statement PS_IMP_PRLS_V4 {
        term IMPORT {
            from {
                route-filter 68.68.0.0/16 orlonger;
                route-filter 207.223.0.0/16 orlonger;
                route-filter 208.51.154.0/24 orlonger;
                route-filter 208.79.0.0/16 orlonger;
                route-filter 208.93.0.0/16 orlonger;
            }
            then accept;
        }
        term REJECT {
            then reject;
        }
    }
    policy-statement PS_IMP_THERMO_V4 {
        term IMPORT {
            from {
                route-filter 10.165.136.0/23 orlonger;
                route-filter 10.166.136.0/23 orlonger;
            }
