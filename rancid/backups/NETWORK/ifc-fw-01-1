    prefix-list PFX_RADIUS-SERVERS_V4 {
        apply-path "system radius-server <*.*.*.*>";
    }
    prefix-list PFX_IPSEC-VPN_PEER_V4 {
        apply-path "security ike gateway <*> address <*.*.*.*>";
    }
    prefix-list PFX_OSPF-MCAST_V4 {
        224.0.0.5/32;
        224.0.0.6/32;
    }
    policy-statement AS_Network {
        term Network {
            from {
                prefix-list Local;
            }
            then accept;
        }
        then reject;
    }
    policy-statement Arkadin_LocalAS_and_backbone {
        term HK-Shared_dmz {
            from {
                prefix-list Local;
            }
            then accept;
        }
        term backbone {
            from {
                instance ARKADIN;
                route-filter 10.0.0.0/8 exact;
                route-filter 10.246.0.0/16 exact;
                route-filter 10.242.0.0/16 orlonger;
                route-filter 10.243.0.0/16 orlonger;
            }
            then accept;
        }
        term default {
            then reject;
        }
    }
    policy-statement Arkadin_backbone {
        term allow {
            from {
                instance ARKADIN;
                route-filter 10.0.0.0/8 exact;
                route-filter 10.246.0.0/16 exact;
                route-filter 10.242.0.0/16 orlonger;
                route-filter 10.243.0.0/16 orlonger;
            }
            then accept;
        }
        term default {
            then reject;
        }
    }
    policy-statement EX_CHQFW01_V4 {
        term 1 {
            from {
                prefix-list-filter PFX_RFC1918_V4 exact;
                prefix-list-filter PFX_PA2_VDED_V4 exact;
                prefix-list-filter PFX_CN_HK_PUBLIC_ONNET_V4 exact;
            }
            then accept;
        }
        term 2 {
            from {
                prefix-list-filter PFX_SH6_V4 exact;
            }
            then accept;
        }
        term default {
            then reject;
        }
    }
    policy-statement EX_CRBFW01_V4 {
        term 1 {
            from {
                prefix-list-filter PFX_CRBFW01_EX_V4 exact;
            }
            then accept;
        }
        term 2 {
            then reject;
        }
    }
    policy-statement IM_CHQFW01_V4 {
        term 1 {
            from {
                neighbor 10.246.255.2;
                prefix-list-filter PFX_CN_INT_AGG_V4 exact;
            }
            then {
                local-preference 120;
                accept;
            }
        }
        term 2 {
            from {
                neighbor 10.246.255.6;
                prefix-list-filter PFX_CN_INT_AGG_V4 exact;
            }
            then {
                local-preference 90;
                accept;
            }
        }
        term 3 {
            then reject;
        }
    }
    policy-statement IM_CRBFW01_V4 {
        term 1 {
            from {
                neighbor 10.246.255.34;
                prefix-list-filter PFX_CRBFW01_IM_V4 exact;
            }
            then {
                local-preference 120;
                accept;
            }
        }
        term 2 {
            from {
                neighbor 10.246.255.38;
                prefix-list-filter PFX_CRBFW01_IM_V4 exact;
            }
            then {
                local-preference 90;
                accept;
            }
        }
        term 3 {
            then reject;
        }
    }
    policy-statement PS_SH6FW01_EXP_V4 {
        term 1 {
            from {
                prefix-list-filter PFX_RFC1918_V4 exact;
                prefix-list-filter PFX_CN_HK_PUBLIC_ONNET_V4 exact;
                prefix-list-filter PFX_ARKAD_PUBLIC_ONNET_V4 exact;
            }
            then accept;
        }
        term default {
            then reject;
        }
    }
    policy-statement PS_SH6FW01_IMP_V4 {
        term 1 {
            from {
                neighbor 10.246.255.26;
                prefix-list-filter PFX_SH6_V4 exact;
                prefix-list-filter PFX_SH6_SHARED_DMZ_ONNET_V4 exact;
            }
            then {
                local-preference 120;
                accept;
            }
        }
        term 2 {
            from {
                neighbor 10.246.255.30;
                prefix-list-filter PFX_SH6_V4 exact;
            }
            then {
                local-preference 90;
                accept;
            }
        }
        term 3 {
            then reject;
        }
    }
    policy-statement default_pref {
        term MX2 {
            from neighbor 172.31.253.30;
            then {
                local-preference 200;
            }
        }
        term default {
            then accept;
        }
    }
    policy-statement localAS_export {
        term 1 {
            from {
                prefix-list PFX_BBONE_EXP;
            }
            then accept;
        }
        term default {
            then reject;
        }
    }
    policy-statement localAS_export_Ppend_x1 {
        term 1 {
            from {
                prefix-list PFX_BBONE_EXP;
            }
            then {
                as-path-prepend 65209;
                accept;
            }
        }
        term default {
            then reject;
        }
    }
    policy-statement ospf-import {
        term deny-CHT-NAT32 {
            from {
                route-filter 10.242.10.1/32 exact;
            }
            then reject;
        }
        term default-accept {
            from {
                route-filter 0.0.0.0/0 orlonger;
            }
            then accept;
        }
    }
}
