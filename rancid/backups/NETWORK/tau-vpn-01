    ARKADIN {
        instance-type virtual-router;
        interface lo0.0;
        interface reth0.4044;
        interface reth0.4048;
        interface reth0.4050;
        interface reth1.102;
        routing-options {
            static {
                route 10.0.0.0/8 next-hop 10.105.10.254;
                route 185.37.223.21/32 next-hop 172.31.253.59;
                route 185.37.223.20/32 next-hop 172.31.253.67;
            }
        }
        protocols {
            bgp {
                graceful-restart;
                group INET {
                    type external;
                    multihop {
                        ttl 16;
                    }
                    local-address 172.31.254.3;
                    neighbor 185.37.223.21 {
                        /* PA2INET01 */
                        import IM_INET_V4;
                        export EX_INET_V4;
                        peer-as 200077;
                    }
                    neighbor 185.37.223.20 {
                        /* LD4INET01 */
                        import IM_INET_V4;
                        export EX_INET_V4;
                        peer-as 200077;
                    }
                }
            }
        }
    }
    ELISA {
        instance-type virtual-router;
