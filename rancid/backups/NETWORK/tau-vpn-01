            encryption-algorithm 3des-cbc;
            lifetime-seconds 28800;
        }
        policy ipsec-phase2-policy-elisa {
            perfect-forward-secrecy {
                keys group2;
            }
            proposals ipsec-phase2-proposal;
        }
        vpn ike-vpn-elisa {
            bind-interface st0.0;
            ike {
                gateway gw-elisa;
                proxy-identity {
                    local 172.31.1.1/32;
                    remote 10.80.147.10/32;
                }
                ipsec-policy ipsec-phase2-policy-elisa;
