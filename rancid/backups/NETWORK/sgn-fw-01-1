    ipsec {
        proposal p2-ipsec-esp-aes256-sha1-3600 {
            protocol esp;
            authentication-algorithm hmac-sha1-96;
            encryption-algorithm aes-256-cbc;
            lifetime-seconds 3600;
        }
        proposal p2-ipsec-esp-aes128-sha1-3600 {
            protocol esp;
            authentication-algorithm hmac-sha1-96;
            encryption-algorithm aes-128-cbc;
            lifetime-seconds 3600;
        }
        proposal p2-ipsec-esp-aes256-sha1-28800 {
            protocol esp;
            authentication-algorithm hmac-sha1-96;
            encryption-algorithm aes-256-cbc;
            lifetime-seconds 28800;
        }
        proposal p2-ipsec-esp-3des-sha1-3600 {
            protocol esp;
            authentication-algorithm hmac-sha1-96;
            encryption-algorithm 3des-cbc;
            lifetime-seconds 28800;
        }
        policy ipsec-p2-apac-offices {
            perfect-forward-secrecy {
                keys group2;
            }
            proposals p2-ipsec-esp-aes256-sha1-3600;
        }
        policy ipsec-p2-ned {
            perfect-forward-secrecy {
                keys group2;
            }
            proposals [ p2-ipsec-esp-3des-sha1-3600 p2-ipsec-esp-aes128-sha1-3600 ];
        }
        policy ipsec-p2-kul {
            perfect-forward-secrecy {
                keys group2;
            }
            proposals [ p2-ipsec-esp-3des-sha1-3600 p2-ipsec-esp-aes128-sha1-3600 ];
        }
        policy ipsec-p2-kul-ericsson {
            proposals p2-ipsec-esp-3des-sha1-3600;
        }
        vpn vpn-nedfw01 {
            bind-interface st0.1;
            ike {
                gateway nedfw01;
                ipsec-policy ipsec-p2-ned;
