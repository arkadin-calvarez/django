            router-id 10.247.20.250;
        }
        protocols {
            bgp {
                graceful-restart;
                group IA_eBGP {
                    type external;
                    multihop {
                        ttl 5;
                    }
                    local-address 172.31.254.2;
                    neighbor 185.37.223.22 {
                        import IA_eBGP_import;
                        /* APACIFCINET01 */
                        export IA_eBGP_export;
                        peer-as 200077;
                    }
                    neighbor 185.37.223.23 {
                        import IA_eBGP_import;
                        /* APACIFCINET02 */
                        export IA_eBGP_export;
                        peer-as 200077;
                    }
                }
                group sinfw01 {
                    type external;
                    export Office_eBGP_export;
                    peer-as 64514;
                    /* LeasedLine */
                    neighbor 172.31.250.2 {
                        hold-time 10;
                    }
                    /* IPsec st0.x */
                    neighbor 172.31.250.6;
                }
            }
        }
    }
