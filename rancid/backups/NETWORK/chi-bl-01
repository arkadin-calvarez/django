firewall {
    family inet {
        filter ACCEPT_GRE_V4 {
            term ACCEPT_GRE_V4 {
                from {
                    protocol gre;
                }
                then {
                    count ACCEPT_GRE_V4;
                    log;
                    accept;
                }
            }
        }
        filter PF_GLOBAL-EDGE_V4 {
            term BLACKLISTED {
                from {
                    source-prefix-list {
                        PFX_BLACKLIST_V4;
                    }
                }
                then {
                    discard;
                }
            }
            term ICMP_FIRST_FRAG {
                from {
                    first-fragment;
                    protocol icmp;
                }
                then {
                    discard;
                }
            }
            term ICMP_FRAGS {
                from {
                    is-fragment;
                    protocol icmp;
                }
                then {
                    discard;
                }
            }
