        filter ACCEPT_BGP_V6 {
            term ACCEPT_BGP_V6 {
                from {
                    source-prefix-list {
                        BGP_NEIGHBORS_V6;
                        BGP_NEIGHBORS_VRF_V6;
                    }
                    destination-prefix-list {
                        LOCAL_IPV6;
                    }
