    policy-statement PS_EXP_VRF-ARKAD-INT {
        term FABRIC_BGP {
            from {
                protocol bgp;
                route-filter 10.127.0.0/16 orlonger;
                route-filter 172.22.0.0/22 orlonger;
                route-filter 192.168.0.0/24 prefix-length-range /32-/32;
                route-filter 10.0.1.4/30 exact;
            }
            then {
                community add COMM_VRF_ARKAD_INT;
                accept;
            }
        }
        term LEGACY_PFXS {
            from {
                protocol bgp;
                community COMM_LEGACY_PFXS;
            }
            then {
                community add COMM_VRF_ARKAD_INT;
                community delete COMM_LEGACY_PFXS;
                accept;
            }
        }
        term LEAK_TO_ARKAD-EXT_V4 {
